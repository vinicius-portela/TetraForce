; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Entity",
"language": "GDScript",
"path": "res://engine/entity.gd"
}, {
"base": "Node2D",
"class": "Item",
"language": "GDScript",
"path": "res://engine/item.gd"
}, {
"base": "StaticBody2D",
"class": "Subitem",
"language": "GDScript",
"path": "res://engine/subitem.gd"
}, {
"base": "Reference",
"class": "entity_helper",
"language": "GDScript",
"path": "res://engine/helper_scipts/entity_helper.gd"
} ]
_global_script_class_icons={
"Entity": "",
"Item": "",
"Subitem": "",
"entity_helper": ""
}

[application]

config/name="TetraForce"
run/main_scene="res://engine/lobby.tscn"
config/icon="res://icon.png"

[autoload]

sfx="*res://engine/sound.gd"
global="*res://engine/global.gd"
network="*res://engine/network.gd"
screenfx="*res://engine/screenfx.tscn"

[debug]

gdscript/warnings/enable=false

[display]

window/size/width=256
window/size/height=144
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
